<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="249.0" prefWidth="524.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ua.prozorro.controller.SchemaExportDialogController">
    <children>
        <GridPane layoutX="10.0" layoutY="30.0" prefHeight="151.0" prefWidth="502.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="139.0" minWidth="10.0" prefWidth="89.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="364.0" minWidth="10.0" prefWidth="364.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="124.0" minWidth="0.0" prefWidth="38.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="-Infinity" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <ComboBox fx:id="comboBoxDB" prefHeight="26.0" prefWidth="376.0" GridPane.columnIndex="1"/>
                <Label text="База данных"/>
                <Label text="Файл" GridPane.rowIndex="2"/>
                <TextField fx:id="textFieldFileName" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Button fx:id="buttonChooseFile" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                        onAction="#onButtonChooseFile" prefWidth="50.0" text="..." GridPane.columnIndex="2"
                        GridPane.rowIndex="2"/>
                <CheckBox fx:id="checkBoxSaveScheme" mnemonicParsing="false" prefHeight="18.0" prefWidth="364.0"
                          text="Создать схему в БД из файла настроек" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
            </children>
        </GridPane>
        <Button fx:id="buttonOk" layoutX="363.0" layoutY="206.0" mnemonicParsing="false" onAction="#onButtonOk"
                prefHeight="26.0" prefWidth="100.0" text="Ok"/>
        <Button fx:id="buttonCancel" layoutX="102.0" layoutY="206.0" mnemonicParsing="false" onAction="#onButtonCancel"
                prefWidth="100.0" text="Cancel"/>
    </children>
</AnchorPane>
